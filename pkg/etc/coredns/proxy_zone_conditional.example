# Corporate internal domain
corp.company.com {
    forward . 192.168.1.10 192.168.1.11
    cache 60
}

# Development domain
dev.local {
    forward . 10.0.0.10
    cache 30
}

# Everything else to public DNS
. {
    forward . 8.8.8.8 8.8.4.4 1.1.1.1 {
        max_fails 3
        expire 10s
        health_check 5s
    }
    cache 300
    log
    errors
}
